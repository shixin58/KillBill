<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_write_1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center"
        android:text="自动Serializable write"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_read_1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_write_1"
        android:gravity="center"
        android:text="自动Serializable read"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_write_2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_read_1"
        android:gravity="center"
        android:text="手动Serializable write"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_read_2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_write_2"
        android:gravity="center"
        android:text="手动Serializable read"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_write_3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_read_2"
        android:gravity="center"
        android:text="Externalizable write"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_read_3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_write_3"
        android:gravity="center"
        android:text="Externalizable read"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_write_4"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_read_3"
        android:gravity="center"
        android:text="Gson write"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_read_4"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_write_4"
        android:gravity="center"
        android:text="Gson read"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_send_1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_read_4"
        android:gravity="center"
        android:text="send Parcelable"
        android:onClick="onClick"/>

    <TextView
        android:id="@+id/tv_send_2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_send_1"
        android:gravity="center"
        android:text="send Externalizable"
        android:onClick="onClick"/>
</androidx.constraintlayout.widget.ConstraintLayout>